{
  "name": "@fennerwe-indigov/crm-models",
  "scripts": {
    "build": "yarn db:generate && tsc && cp -r ./src/generated ./dist/",
    "db:generate": "yarn pnpify prisma generate",
    "db:create-migration": "yarn pnpify prisma migrate dev --name",
    "db:deploy-migrations": "prisma migrate deploy",
    "format": "prettier ./src --write && yarn prisma format",
    "db:seed": "ts-node ./prisma/seed/seedData.ts"
  },
  "main": "./dist/index.js",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^8.4.1",
    "@types/argparse": "^2",
    "@types/eslint": "^9",
    "@types/node": "^22.0.0",
    "@yarnpkg/pnpify": "^4.1.0",
    "argparse": "^2.0.1",
    "date-fns": "^3.6.0",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "prisma": "5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "packageManager": "yarn@4.3.1",
  "dependencies": {
    "@prisma/client": "5.17.0"
  }
}
