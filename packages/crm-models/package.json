    "@types/node": "^20.14.12",
    "@yarnpkg/pnpify": "^4.1.0",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "prisma": "5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "packageManager": "yarn@4.3.1",
  "dependencies": {
    "@prisma/client": "5.17.0"
  }
}
